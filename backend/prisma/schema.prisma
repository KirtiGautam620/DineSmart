generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Favorites{
  id Int @id @default(autoincrement())
  userId Int 
  recipeId Int
  title String
  image String
  cookTime String
  servings String
  createdAt DateTime @default(now())

  @@unique([userId, recipeId])
}
